  >>> ';'.join(list)  
'first;second;third'  
>>> ';'.join(list).split(';')  
['first', 'second', 'third']  








>>> a = [1,2,2,3,5,8,10,10,20]
>>> b = [3,3,3,3,10,11,1,1,1]
>>> set(a)
set([1, 2, 3, 5, 8, 10, 20])
>>> c = set(a) & set(b)
>>> c
set([1, 10, 3])
>>> set(b)
set([11, 1, 10, 3])
>>> d = list(c)
>>> d
[1, 10, 3]
>>> d.sort(key = a.index)
>>> d
[1, 3, 10]
>>>
>>> d.sort(key = b.index)
>>> d
[3, 10, 1]








